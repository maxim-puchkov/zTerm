<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.maximpuchkov.services.Update</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/update-service</string>
		<string>/usr/local/bin/brew</string>
		<string>/usr/local/bin/tldr</string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Weekday</key>
			<integer>1</integer>
		</dict>
	</array>
	<key>WorkingDirectory</key>
	<string>/usr/local/Terminal</string>
	<key>StandardOutPath</key>
	<string>var/log/com.maximpuchkov.services/Update.log</string>
	<key>StandardErrorPath</key>
	<string>var/log/com.maximpuchkov.services/Update.log</string>
</dict>
</plist>
