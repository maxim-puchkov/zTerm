#compdef list

# _list
# ~/Developer/Projects/Terminal/zsh/functions/list
#
# zsh function completion
# Updated on 2020-06-07 at 05:21


_arguments -C \
  "(-)"{-h,--help}"display help information" \
  ":command:->command" \
  "*::arg:->arg"
  

case $state in
  (command)
    typeset -a commands
    commands=( 
      f:"file flags"
      tc:"time of creation"
      tch:"time of change"
      tm:"time of modification"
      S:"file size"
      ta:"time of access"
    )
    _describe -t commands command commands
#    return 0
  ;;
  (arg)
    typeset -a args
    args=(
    
    )
    _describe -t args arg args
#    return 0
  ;;
esac
return 0
