#compdef set-system-appearance


function _set-system-appearance_commands() {
  typeset -a fn_commands
  fn_commands=(
    light:"Light system appearance"
    dark:"Dark system appearance"
  )
  _describe command fn_commands
  return 0
}

_set-system-appearance() {
  _arguments \
    '1: :_set-system-appearance_commands'
  return 0
}

_set-system-appearance "$@"
return 0
