#compdef foo


_arguments -C -s -A "-*" -S \
  '-a[desc of a]' \
  '-b[desc of b]' \
  '(-a -b)-c[exclude a and b]' \
  '(-c)-d[exclude c and show directory]:directory:_directories' \
  '(-a -b -c)'{-e,--echo}'[exclude a b c and show nothing]:nothing' \
  '-f[file descriptor]:file_descriptor:_file_descriptors' \
  '*: :_functions' \
  && ret=0


return 0
