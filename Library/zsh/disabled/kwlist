# Print input arguments as a list of
# enumerated keywords.
function kwlist() {
    if [[ $# -eq 0 ]]; then
        red "No keywords"
        return 1
    fi
    
    for i in {1..$#}; do
        echo "$i. ${@[$i]}"
    done
    return 0
}
