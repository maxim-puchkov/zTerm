# score
#
score () {
	integer value=${1:=1} 
	integer maxvalue=${2:=5} 
	integer not_filled filled
	let filled="$maxvalue-$value"
	let not_filled="$maxvalue-$filled"
	print ${(epr:$not_filled::●:)}${(pr:$filled::○:)}
}
