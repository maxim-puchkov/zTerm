# <#Description...#>
#
# Usage:
# zfn
# <#usage...#>
emulate -LR zsh


# Function options
## -x :: Option with default value
## -y :: Option
local -A opts
opts=( [-x]='default value' )
local -a specs
specs=( ${(k)^opts#-}':' )
if [[ $# -gt 0 ]] &&
   {! zparseopts -D -K -M -A opts - $specs}; then
  return 1
fi


local subcommand="${1:=help}"
shift
if [[ ${+functions[${0}_${subcommand}]} -eq 0 ]]; then
  error -127 -m 'no such command: ${subcommand}'
fi
${0}_${subcommand} ${argv}
