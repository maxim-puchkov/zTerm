#!/bin/zsh

#  takef
#  /usr/local/zterm/zsh/functions (id: 99)
#
#  Generated by 'ztemplate' (zterm/zsh/sources) on 2020-10-17
#  Copyright Â© 2020 Maxim Puchkov. All rights reserved.


# takef options
## -n: Do not open files
local -A opts
opts=()
local -a specs
specs=('n')
if [[ $# -gt 0 ]] &&
   {! zparseopts -D -E -K -M -A opts - $specs}; then
  return 1
fi


# Create files that do not exist.
local file
for file; do
  if [[ ! -e $file ]]; then
    ztemplate -zsh -file-header >$file
  fi
done


# Open files unless '-n' is set.
if [[ ${+opts[-n]} -eq 0 ]]; then
  /usr/bin/open -a Xcode -- $argv
fi
return 0
