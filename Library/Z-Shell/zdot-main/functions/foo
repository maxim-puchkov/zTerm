function a() {
  unset opts
  typeset -Ag opts
  local Z=${Z:-'-D -E -F -K -M'}
  local S=${S:-'a b: c:: d+:: -e:: f g-::-'}
  zparseopts \
    $=Z      \
    -A opts  \
    -        \
    $=S
  show opts
  
}


local cmd=$1
shift
$cmd $argv

