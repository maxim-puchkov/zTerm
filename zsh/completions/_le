#compdef le

# _le
#
#
# zsh function completion
#

typeset -a args=()

args=(
    "-n[create an empty filename (the disk blocks aren't allocated until data is written to them)]"
    "-v[report the names and sizes of created files]"
    '+a[do not capitalize first letter]'
'--no-autocapitalize[do not capitalize first letter]'
'-t[set error message title]'
'--title[set error message title]'
'-n[try to silence output messages]'
'--null[try to silence output messages]'
'-D[do not format description]'
'--plain-description[do not format description]'
'-a[capitalize first letter]'
'--autocapitalize[capitalize first letter]'
'-u[set title to '''Usage''']'
'--usage[set title to '''Usage''']'
'-c[show error exit code]'
'--code[show error exit code]'
)

#_arguments -s -S $args
_arguments -s $args     '-E[specify EOF marker]:end-of-file marker'     '(-x -i)-I[specify replacement string for command line]:replacement string'     '(-n -l)-L[call program for every number of lines]:number of input lines'     '(-)1: :(b k m g)'     '*::args: _normal'


return 0
