local -a reply
_regex_words mydb-commands 'mydb commands' \
    'add:add an entry to mydb:$mydb_add_cmds' \
    'show:show entries in mydb'
_regex_arguments _mydb "$reply[@]"
_mydb "$@"

compdef _mydb       foo
