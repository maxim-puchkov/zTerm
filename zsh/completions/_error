#compdef error

# _error
# ~/Developer/Projects/Terminal/zsh/functions/error
#
# zsh function completion
# Updated on 2020-06-25 at 06:59 pm


_arguments -C \
  {+A,--no-autocapitalize}"[do not capitalize first letter]" \
  {+H,--no-highlight}"[disable syntax highlighting]" \
  {+P,--no-trailing-period}"[do not add trailing period]" \
  {+S,--no-split-lines}"[do not capitalize first letter]" \
  {-A,--autocapitalize}"[capitalize first letter]" \
  {-D,--plain-description}"[do not format description]" \
  {-H,--highlight}"[enable syntax highlighting]" \
  {-P,--trailing-period}"[add trailing period]" \
  {-S,--split-lines}"[capitalize first letter]" \
  {-c,--code}"[display exit code with error message]" \
              -e"[set the exit code]" \
  {-h,--help}"[show help message]" \
  {-n,--null}"[try to silence output messages]" \
  {-t,--title}"[set error message title]" \
  {-u,--usage}"[set title to 'Usage']" \
  ":command:->command" \
  "*::arg:->arg"

case $state in
  (command)
    typeset -a commands
    commands=(

    )
    _describe -t commands command commands
  ;;
  (arg)
    typeset -a args
    args=(
      # Completion for commands' arguments
    )
    _describe -t args arg args
  ;;
esac

return 0
