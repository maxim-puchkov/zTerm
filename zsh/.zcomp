# Initialize the completion system.
autoload -U compinit
compinit


#MARK: - Completions
# Aliases
compdef _alias          find-alias
compdef _alias          alias_value

# Homebrew
compdef _brew_formula   brew_browse

# Shell builtin commands help
compdef _builtin        help

# Python Package Installer
compctl -K _pip         pip  pip3  pip3.7  pip3.8

# Commands
compdef _command        syntax  brief

#
compdef _command        hist

# Processes
compdef _pids           leaks
compdef _pids           lskq
compdef _pids           malloc_history

# Symbolic linker
compdef _sln            sln

#
compdef _nothing        update.sh
compdef _nothing        spectrum  color_scheme

#
compdef _vars           try  tryregex
compdef _vars           typeof

# Navigation
compdef _cd             cdp
