# User-specific zsh script loader.


source "$ZEXPORT"


# PATH
path=(
  /usr/local/sbin
  /usr/local/opt/python@3/bin
  /usr/local/opt/llvm/bin
  $path
  $DEV/bin
  $PROJ/Terminal/.config/bin
  $pythonpath
  #$PROJ/Terminal/Python
)

export PATH="/usr/local/opt/openssl@1.1/bin:$PATH"

# FPATH
fpath=(
  /usr/local/share/zsh-completions
  $PROJ/Terminal/zsh/functions
  $PROJ/Terminal/zsh/completions
  $fpath
)

# Load functions
autoload fload
fload

# Source files
source "$ZIO"
source "$ZALIAS"
source "$ZUTIL"
source "$ZNET"
source "$ZMISC"
source "$ZCOMP"

source '/usr/local/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh'
source '/usr/local/share/zsh-autosuggestions/zsh-autosuggestions.zsh'
typeset -H _ZSH_AUTOSUGGEST_BIND_COUNTS
typeset -H ZSH_HIGHLIGHT_STYLES
typeset -H ZSH_AUTOSUGGEST_ACCEPT_WIDGETS
typeset -H ZSH_AUTOSUGGEST_PARTIAL_ACCEPT_WIDGETS
typeset -H ZSH_AUTOSUGGEST_IGNORE_WIDGETS
typeset -H ZSH_HIGHLIGHT_REVISION
typeset -H ZSH_AUTOSUGGEST_CLEAR_WIDGETS

unalias run-help
autoload run-help

unalias g
