# User-specific zsh script loader.


source "$ZEXPORT"


# PATH
path=( "/usr/local/sbin" $path  )
path=( "/usr/local/opt/python@3/bin" $path)
path=( "/usr/local/opt/llvm/bin" $path )
path=( $path "$HOME/Developer/bin" )
path=( $path "$DEV/Terminal/.config/bin" )
path=( $path "$DEV/Terminal/Python" )
#path=( $path "$SFU/bin" )


# Function path
fpath=( "/usr/local/share/zsh-completions" $fpath )
fpath=( "$DEV/Terminal/zsh/functions" $fpath )
fpath=( "$DEV/Terminal/zsh/completions" $fpath )


autoload fload
fload


# Source files
source "$ZIO"
source "$ZALIAS"
source "$ZUTIL"
source "$ZNET"
source "$ZMISC"
source "$ZCOMP"

source "/usr/local/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh"
source "/usr/local/share/zsh-autosuggestions/zsh-autosuggestions.zsh"


unalias run-help
autoload run-help
