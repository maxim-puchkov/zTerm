# User-specific zsh script loader.


source "$ZEXPORT"


# PATH
path=(
  /usr/local/sbin
  /usr/local/opt/python@3/bin
  /usr/local/opt/llvm/bin
  $path
  $DEV/bin
  $XCDEV/Terminal/.config/bin
  $XCDEV/Terminal/Python
  # $SFU/bin
)


# FPATH
fpath=(
  /usr/local/share/zsh-completions
  $XCDEV/Terminal/zsh/functions
  $XCDEV/Terminal/zsh/completions
  $fpath
)
autoload fload
fload


# Source files
source "$ZIO"
source "$ZALIAS"
source "$ZUTIL"
source "$ZNET"
source "$ZMISC"
source "$ZCOMP"

source "/usr/local/share/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh"
source "/usr/local/share/zsh-autosuggestions/zsh-autosuggestions.zsh"


unalias run-help
autoload run-help

unalias g
