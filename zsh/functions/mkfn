#!/bin/zsh

#  mkfn
#  Generated by mkzsh at 12:36am.
#
#  Created by mpuchkov/506:20 on April 17, 2021.
#  ~Projects/zTerm2/zsh/functions/mkfn


emulate -L zsh
autoload -Uz mkzsh error
if [[ $# -eq 0 ]]; then
  error -1 -m 'not enough arguments'
fi

local name
for name; do
  mkzsh "$ZDOTDIR/functions/$name"
done
