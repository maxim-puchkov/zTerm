# Change directory and print its path.
function latex() {
    symbols=('alpha' 'beta' 'gamma' 'delta' 'epsilon'
        'zeta' 'eta' 'theta' 'iota' 'kappa' 'lambda'
        'mu' 'nu' 'xi' 'pi' 'rho' 'sigma' 'tau'
        'upsilon' 'phi' 'chi' 'omaga')
    for s in $symbols; do
        printf '%s: %s\n' "$s" "\\$s"
    done
}

