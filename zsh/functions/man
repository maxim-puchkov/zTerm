  LESS_TERMCAP_md=$'\e[96m'   \
  LESS_TERMCAP_me=$'\e[0m'    \
  LESS_TERMCAP_so=$'\e[1;7m'  \
  LESS_TERMCAP_us=$'\e[4;32m' \
  LESS_TERMCAP_ue=$'\e[0m'    \
