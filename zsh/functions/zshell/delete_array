# Delete saved array data.
function delete_array() {
    local identifier="$1"
    local file="$DEV/Library/Data/$identifier.array"
    if [[ ! -r $file ]]; then
        printf 'Not a saved array: %s.\n' "$(Bad $identifier)" >&2
        return 1
    fi
    printf '' > $file 
    return 0
}
