# <#Function description#>
function find-alias() {
    local alias_id="$1"
    if [[ -z "$alias_id" ]]
    for fsrc in $functions_source; do
        grep 'alias lsa' $fsrc;
    done
    printf 'Defined in: %s.\n' $(blue "$def")
    return 0
}
