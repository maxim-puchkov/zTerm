# <#Function description#>
function cats() {
  local input
  while [[ -n $1 ]]; do
    print-line "TEXT"
    cat <$1
    printf '\n(Press return to continue...\n'
    read -sk input
    if [[ $input != $'\n' ]]; then break; fi
    shift
  done
}
