# <#Function description#>
function ___FILENAME___() {
    printf 'Template z-function: %s.\n' $(blue "$0")
    
    # Print to stderr
    local def=$(fn src $0)
    if [[ $? -ne 0 ]]; then
        printf 'Definition not found.\n' >&2
        return 1
    fi
    
    printf 'Defined in: %s.\n' $(blue "$def")
    return 0
}
