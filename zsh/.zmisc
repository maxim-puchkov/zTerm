function newlines() {
    typeset -a nl=(\n \\n '\n' '\\n' $'\n' "\n" "\\n" $(echo) $(printf '\n') '
')
    typeset -i i=1
    while [[ $i -lt $#nl ]]; do
        printf '--> %d:\nS:<%s>,\tB:<%b>,\tH:<\n%s>\n' $i "${nl[$i]}" "${nl[$i]}" "$(hexyl <<< $nl[$i])"
        let i=$i+1
    done
}
